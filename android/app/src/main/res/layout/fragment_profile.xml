<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/profile_my_page_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="마이 페이지"
        android:textSize="18sp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/profile_completed_classes_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="수강완료 과목"
        android:textSize="18sp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toEndOf="@id/profile_my_page_tv"
        app:layout_constraintTop_toTopOf="parent"/>

    <FrameLayout
        android:id="@+id/profile_vp"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@id/profile_my_page_tv" />




</androidx.constraintlayout.widget.ConstraintLayout>